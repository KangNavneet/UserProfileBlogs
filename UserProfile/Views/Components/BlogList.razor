@using UserProfile.Models
<h3>Blog List</h3>
<table border="5">
   
        @foreach (BlogModel blog in ModelList)
        {
            <tr>
            <td>@blog.Name</td>
            <td>@blog.Slug</td>
            </tr>
        }
    

</table>

@code {

    [Parameter]
    public IEnumerable<BlogModel> ModelList { get; set; }


}
